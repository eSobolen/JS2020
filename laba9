<html>
	 <head>
	     <meta charset='utf-8' />
	 </head>
	    <body>
	        <script>
	         var Data = new Date();
	         var Hour = Data.getHours();
	         var Minutes = Data.getMinutes();
	         alert ('Текущее время: ' +  Hour + ":"  + Minutes );  
	         var time = Hour + Minutes / 60;
	
	         if (time >= 7 && time < 8){
	              alert ('завтрак');
	            }
	
	         if (time >= 13 && time < 14){
	              alert ('обед');
	            }
	
	         if (time >= 19 && time < 20){
	              alert ('ужин');
	            }
	
	         if (time >= 9 && time < 18){
	               alert ('работа');
	            }  
	
	         if (time >= 18 && time < 23){
	          alert ('отдых');
	        }   
	
	         if (time >=  23 || time < 6.5 ) { 
	             alert ('сон'); 
	            }
	        </script>
	  </body>
	</html>
