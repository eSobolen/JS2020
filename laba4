<html>
	 <head>
	     <meta charset='utf-8' />
	 </head>
	    <body>
	        <script>
	         var y = prompt('введите текст ');
	         var z = y.split('');
	         var g = z[4];
	         var t = 0;
	         for ( var x = 0 ; x < 21 ; x++ ) {    
	               if (z[x] == g){
	               t++;
	       }
	       }
	          alert ('в вашей строке ' +  t  + ' пятых символов ');  
	        </script>
	  </body>
	</html>
