<head>
	    <meta charset="utf-8" />
	</head>
	
	<body>
	  
	
	    <div id="txt"></div>
	    <script type='text/javascript'>
	        var n = prompt('Enter n');
	        var arr = [];
	
	        function getRandomInt(min, max) {
	            return Math.floor(Math.random() * (max - min + 1)) + min;
	        }
	
	        for (var i = 0; i < n; i++) {
	            var a = getRandomInt(0, 10);
	            arr.push(a);
	        }
	        alert(arr);
	    </script>
	</body>
	
	</html>
